#!/bin/sh

files="lib.js out.js rts.js"

git checkout gh-pages || exit 1
for file in $files; do
    cp ./dist-newstyle/build/x86_64-linux/ghcjs-8.6.0.1/cat-0.1.0.0/x/cat/build/cat/cat.jsexe/$file . || exit 1
done
# ccjs all.js --compilation-level=ADVANCED_OPTIMIZATIONS --jscomp_off=checkVars --externs=node --externs=all.js.externs > all.min.js
git add $files
git commit -m 'Deployed site'
git push
git checkout master

